{
  "name": "irwan-runtuwene/driver-whatsapp",
  "license": "MIT",
  "description": "WABA On-Premise driver for BotMan",
  "keywords": [
      "Bot",
      "BotMan",
      "WhatsApp",
      "WABA"
  ],
  "homepage": "http://github.com/irwan-runtuwene/driver-whatsapp",
  "authors": [
      {
          "name": "S. Irwan Runtuwene",
          "email": "stefanus.irwant@gmail.com"
      }
  ],
  "require": {
      "php": ">=7.0",
      "botman/botman": "^2.0",
      "ext-curl": "*"
  },
  "require-dev": {
      "botman/studio-addons": "^1.0",
      "illuminate/contracts": "^5.5",
      "phpunit/phpunit": "^5.0",
      "mockery/mockery": "^1.1"
  },
  "autoload": {
      "psr-4": {
          "BotMan\\Drivers\\Whatsapp\\": "src/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
      }
  },
  "scripts": {
      "test": "vendor/bin/phpunit",
      "cs": "php-cs-fixer fix"
  },
  "extra": {
      "laravel": {
          "providers": [
              "BotMan\\Drivers\\Whatsapp\\Providers\\WhatsappServiceProvider"
          ]
      }
  }
}